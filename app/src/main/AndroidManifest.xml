        <activity android:name=".BasicWidget.DatePickerActivity" />
        <activity android:name=".BasicWidget.CompoundWidgetActivity" />
        <activity android:name=".BasicWidget.CompoundWidgetTestActivity" />
        <activity android:name=".ListView.ArrayAdapterActivity" />
